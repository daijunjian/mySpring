<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dale.elec.domain.ElecText" table="elec_text">
		<id name="textID" type="string">
			<column name="textID"/>
			<generator class="uuid"></generator>
		</id>
		<property name="textName" type="string">
			<column name="textName"/>
		</property>
		<property name="textDate" type="date">
			<column name="textDate"/>
		</property>
		<property name="textRemark" type="string">
			<column name="textRemark"/>
		</property>
	</class>
</hibernate-mapping>
