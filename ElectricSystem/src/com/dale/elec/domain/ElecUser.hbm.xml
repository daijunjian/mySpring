<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dale.elec.domain.ElecUser" table="elec_user">
		<id name="userID" type="string">
			<column name="userID"/>
			<generator class="uuid"></generator>
		</id>
		<property name="jctID" type="string">
			<column name="jctID"/>
		</property>
		<property name="userName" type="string">
			<column name="userName"/>
		</property>
		<property name="logonName" type="string">
			<column name="logonName"/>
		</property>
		<property name="logonPwd" type="string">
			<column name="logonPwd"/>
		</property>
		<property name="sexID" type="string">
			<column name="sexID"/>
		</property>
		<property name="birthday" type="date">
			<column name="birthday"/>
		</property>
		<property name="address" type="string">
			<column name="address"/>
		</property>
		<property name="contactTel" type="string">
			<column name="contactTel"/>
		</property>
		<property name="email" type="string">
			<column name="email"/>
		</property>
		<property name="mobile" type="string"> 
			<column name="mobile"/>
		</property>
		<property name="isDuty" type="string">
			<column name="isDuty"/>
		</property>
		<property name="onDutyDate" type="date">
			<column name="onDutyDate"/>
		</property>
		<property name="offDutyDate" type="date">
			<column name="offDutyDate"/>
		</property>
		<property name="remark" type="string">
			<column name="remark"/>
		</property>
	</class>
</hibernate-mapping>